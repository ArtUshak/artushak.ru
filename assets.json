{
    "assets": {
        "favicon-svg": {
            "extension": "svg",
            "source": {
                "File": "images/favicon.svg"
            }
        },
        "favicon": {
            "output_base_path": "images",
            "extension": "svgz",
            "source": {
                "Filtered": {
                    "filter_name": "RunExecutable",
                    "input_names": [
                        "favicon-svg"
                    ],
                    "options": {
                        "executable_name": {
                            "String": "gzip"
                        },
                        "extra_arguments": {
                            "StringList": [
                                "--to-stdout"
                            ]
                        },
                        "output_is_stdout": "Flag"
                    }
                }
            }
        },
        "style-scss": {
            "extension": "scss",
            "source": {
                "File": "styles/style.scss"
            }
        },
        "style": {
            "output_base_path": "styles",
            "extension": "css",
            "source": {
                "Filtered": {
                    "filter_name": "SCSS2CSS",
                    "input_names": [
                        "style-scss"
                    ],
                    "options": {}
                }
            }
        },
        "khandamirov-artiom-photo1": {
            "output_base_path": "images",
            "extension": "jpg",
            "source": {
                "File": "images/khandamirov-artiom-photo1.jpg"
            }
        }
    },
    "public_assets": [
        "favicon",
        "style",
        "khandamirov-artiom-photo1"
    ]
}